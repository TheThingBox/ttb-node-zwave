{
  "name": "ttb-node-zwave",
  "version": "2.0.3",
  "description": "A set of Node-RED nodes for ZWave support on the Thingbox (http://TheThingbox.io) and TimeSquAir (http://TimeSquAir.io)",
  "node-red": {
    "nodes": {
      "zwave": "nodes/zwave.js",
      "zwave-generic": "nodes/zwave-generic.js",
